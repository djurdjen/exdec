{
  "name": "node-vue-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npx nodemon ./bin/www localhost 3000",
    "dev": "vue-cli-service serve --port 8080",
    "seed": "node ./seed/init",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "date-and-time": "^0.6.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.1.1",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.1",
    "nodemon": "^1.18.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.32.2",
    "sequelize-cli": "^5.4.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.14.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^3.0.5",
    "eslint-plugin-only-warn": "^1.0.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "vue",
      "only-warn"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
